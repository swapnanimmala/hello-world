pipeline{
    agent {label 'java'}
    stages('CI'){
        stage('checkout'){
            steps{
                echo 'checkout stage'
                checkout scm
            }
        }
        stage('build'){
            steps{
                echo 'Build stage'
                sh 'mvn clean install'
            }
        }
        stage('test'){
            steps{
                echo 'Test cases '
                sh 'mvn test'
            }
        }
    }
}
